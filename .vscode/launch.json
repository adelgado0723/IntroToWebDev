  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "processId": "${command.PickProcess}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch",
      "cwd": "${workspaceRoot}/server.js",
      "program": "${workspaceRoot}/server.js"
      //   "url": "http://localhost:8080",
      //   "webRoot": "${workspaceFolder}"
    }
  ]
}
