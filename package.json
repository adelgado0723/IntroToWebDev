  "bugs": {
    "url": "https://github.com/adelgado0723/CSS_Grid/issues"
  },
  "homepage": "https://github.com/adelgado0723/CSS_Grid#readme",
  "devDependencies": {
    "browser-sync": "^2.24.5",
    "run-script-os": "^1.0.3"
  }
}
